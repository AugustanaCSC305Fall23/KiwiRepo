<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="150.0" prefWidth="200.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="edu.augustana.ChoosePlanController">
   <children>
      <Label layoutX="22.0" layoutY="39.0" text="Choose Plan To Be Printed" textAlignment="CENTER">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
      <ChoiceBox fx:id="choosePlanChoiceBox" layoutX="25.0" layoutY="63.0" prefWidth="150.0" />
      <Button fx:id="printSelectedPlanButton" layoutX="28.0" layoutY="104.0" mnemonicParsing="false" onAction="#printLessonPlanImages" text="Print" />
      <Button fx:id="previewBtn" layoutX="88.0" layoutY="104.0" mnemonicParsing="false" onAction="#switchToPrintView" text="See Preview" />
      <Button fx:id="backBtn" layoutX="14.0" layoutY="14.0" mnemonicParsing="false" onAction="#switchToCreateAPlan" text="Back" />
   </children>
</AnchorPane>
